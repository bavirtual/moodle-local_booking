{
    "integrations": {
        "pireps": {
            "enabled": 1,
            "host": "127.0.0.1:3306",
            "db": "bav_management",
            "data": {
                "pirepinfo": {
                    "table": "pireps",
                    "fields": {
                        "pilot_id": "pilot_id",
                        "flightdate": "start_date",
                        "flighttime": "flight_time",
                        "depicao": "dep_icao",
                        "arricao": "arr_icao",
                        "deptime": "start_time",
                        "arrtime": "end_time",
                        "callsign": "callsign",
                        "aircraft": "aircraft",
                        "route": "planned_route",
                        "aircraftreg": "registration",
                        "fstd": "sim",
                        "linkedpirep": "linked_pirep_id"
                    },
                    "primarykey": "id"
                }
            }
        },
        "aircraft": {
            "enabled": 1,
            "host": "localhost",
            "db": "bav_management",
            "data": {
                "aircraftinfo": {
                    "table": "fleet",
                    "fields": {
                        "engine_type": "engine_type",
                        "description": "description"
                    },
                    "primarykey": "icao"
                }
            }
        },
        "pilots": {
            "enabled": 1,
            "host": "localhost",
            "db": "bav_management",
            "data": {
                "pilotinfo": {
                    "table": "pilots",
                    "fields": {
                        "alternatename": "baw_id"
                    },
                    "primarykey": "id"
                }
            }
        }
    },
    "pdftkpath": "/usr/local/bin/pdftk"
}